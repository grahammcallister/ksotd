@{
    Layout = null;
}
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8"/>
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1"/>
    <title>Keyboard shortcut of the day</title>
    <link href="Content/bootstrap.min.css" rel="stylesheet">
    <!-- IE10 viewport hack for Surface/desktop Windows 8 bug -->
    <link href="css/bootstrap_custom/ie10-viewport-bug-workaround.css" rel="stylesheet">
    <link href="css/bootstrap_custom/sticky-footer.css" rel="stylesheet">

    <link href="css/keyscss/keys.css" rel="stylesheet">
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.3/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
</head>
<body>
<div class="container" role="main">
    <div class="row">
        <h4>Keyboard Shortcut of the Day</h4>
    </div>
    <div class="row jumbotron">
        <h2>
            <span class="label label-primary">Move Code Up</span> - <span class="label label-default">Jetbrains Resharper 2016</span>
        </h2>
        <h1>
            <kbd class="light">
                Ctrl
            </kbd>
            &nbsp;+&nbsp;
            <kbd class="light">
                Shift
            </kbd>
            &nbsp;+&nbsp;
            <kbd class="light">
                Alt
            </kbd>
            &nbsp;+&nbsp;
            <kbd class="light">
                Up
            </kbd>
        </h1>
    </div>
    <div class="row">
        <span>Learn a keyboard shortcut every day</span>
    </div>
</div>
    <footer class="footer">
        <div class="container">
            <p class="text-muted">Helping developers and power users to learn new keyboard shortcuts for their favourite tools. Accepting pull requests on Github: <a href="https://github.com/grahammcallister/ksotd">https://github.com/grahammcallister/ksotd</a></p>
        </div>
    </footer>
<script src="Scripts/jquery-1.9.1.min.js"></script>
<script src="Scripts/bootstrap.min.js"></script>
</body>
</html>
